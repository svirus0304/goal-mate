package com.GoalMate.www.DTO;

public class FreeCommenDTO {
	private int cf_num;//댓글 번호
	private String cf_content;//댓글 내용
	private String cf_id; //댓글 작성자
	private String cf_join; //댓글 작성일
	private int bod_num;//게시판 번호
	private int bod_rcount;//댓글수
	private String cf_type;//댓글 계정타입
	
	
	public String getCf_type() {
		return cf_type;
	}
	public void setCf_type(String cf_type) {
		this.cf_type = cf_type;
	}
	public int getBod_rcount() {
		return bod_rcount;
	}
	public void setBod_rcount(int bad_rcount) {
		this.bod_rcount = bad_rcount;
	}
	public int getCf_num() {
		return cf_num;
	}
	public void setCf_num(int cf_num) {
		this.cf_num = cf_num;
	}
	public String getCf_content() {
		return cf_content;
	}
	public void setCf_content(String cf_content) {
		this.cf_content = cf_content;
	}
	public String getCf_id() {
		return cf_id;
	}
	public void setCf_id(String cf_id) {
		this.cf_id = cf_id;
	}
	public String getCf_join() {
		return cf_join;
	}
	public void setCf_join(String cf_join) {
		this.cf_join = cf_join;
	}
	public int getBod_num() {
		return bod_num;
	}
	public void setBod_num(int bod_num) {
		this.bod_num = bod_num;
	}
	
}
