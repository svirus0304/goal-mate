package com.GoalMate.www.DTO;

public class ScoreDTO {
	int mat_num;	//매치 번호
	String re_home_score; // 홈점수
	String re_away_score; // 어웨이점수
	String re_path1; //인증샷 경로1
	String re_path2; //인증샷 경로2
	String mat_confirm; //관리자 승인 여부
	public int getMat_num() {
		return mat_num;
	}
	public void setMat_num(int mat_num) {
		this.mat_num = mat_num;
	}
	public String getRe_home_score() {
		return re_home_score;
	}
	public void setRe_home_score(String re_home_score) {
		this.re_home_score = re_home_score;
	}
	public String getRe_away_score() {
		return re_away_score;
	}
	public void setRe_away_score(String re_away_score) {
		this.re_away_score = re_away_score;
	}
	public String getRe_path1() {
		return re_path1;
	}
	public void setRe_path1(String re_path1) {
		this.re_path1 = re_path1;
	}
	public String getRe_path2() {
		return re_path2;
	}
	public void setRe_path2(String re_path2) {
		this.re_path2 = re_path2;
	}
	public String getMat_confirm() {
		return mat_confirm;
	}
	public void setMat_confirm(String mat_confirm) {
		this.mat_confirm = mat_confirm;
	}
	
}
