@charset "UTF-8";
/* Starter CSS for Flyout Menu */
#leftmenu {
  padding: 0;
  margin: 0;
  border: 0;
}
#leftmenu ul,
#leftmenu ul li,
#leftmenu ul ul {
  list-style: none;
  margin: 0;
  padding: 0;
}
#leftmenu ul {
  position: relative;
  z-index: 597;
  float: left;
  width: 200px;
}
#leftmenu ul li {
  float: left;
  min-height: 1px;
  line-height: 1em;
  vertical-align: middle;
}
#leftmenu ul li.hover,
#leftmenu ul li:hover {
  position: static;/* relative here */ /* 2단메뉴 나타나는 위치 */
  z-index: 599;
  cursor: default;
}
#leftmenu ul ul {
  visibility: hidden;
  position: absolute;
  top: 100%;
  left: 0;
  z-index: 598;
  width: 0%;/* here 100% */
}
#leftmenu ul ul li {
  float: none;
}
#leftmenu ul ul ul {
  top: 1px;
  left: 99%;
}
#leftmenu ul li:hover > ul {
  visibility: visible;
}
#leftmenu ul ul {
  top: 1px;
  left: 99%;
}
#leftmenu ul li {
  float: none;
}
#leftmenu ul {
  font-weight: bold;
}
#leftmenu ul ul {
  margin-top: 1px;
}
#leftmenu ul ul li {
  font-weight: normal;
}
/* Custom CSS Styles */
#leftmenu {
  border-radius: 5px 0 0 5px;
  -moz-border-radius: 5px 0 0 5px;
  -webkit-border-radius: 5px 0 0 5px;
  background-color: #333333;
  /* background-image: url(data:image/png;base64,); */
  /* background-repeat: repeat-x; */
  width: 200px;
}
#leftmenu:after {
  content: '';
  display: block;
  clear: both;
}

/* left 글씨, 배경 설정 */
#leftmenu a {
  background-color: #333333;/* 배경색깔 */
  /* background-image: url(data:image/png;base64,); */
  /* background-repeat: repeat-x; */
  color: #ffffff;	/* 평소 글자색 */
  display: block;
  font-family: 굴림,Helvetica, Arial, Verdana, sans-serif;	/* 글씨체 */
  font-size: 20px;	/* 글자 크기 */
  font-weight: bold;
  line-height: 300%;	/* 메뉴 높이 */
  padding: 13px 20px;	/* 글자와 메뉴 양쪽 끝 간격 */
  text-decoration: none;
}
/* 메뉴판 겉에 있는 줄 */
#leftmenu ul {
  /* border-right: 2px solid #0fa1e0;	메뉴판 오른쪽에 있는 줄 */	
  /* list-style: none; */
}
#leftmenu > ul {
  float: left;
}
/*  */
#leftmenu > ul > li:hover:after {
  content: '';
  display: block;
  width: 0;
  height: 0;
  position: absolute;
  right: 0;
  top: 50%;
  border-top: 10px solid transparent;
  border-bottom: 10px solid transparent;
  border-right: 10px solid #333333;	/* hover 시 화살표 */
  margin-top: -10px;
}
#leftmenu > ul > li:first-child > a {
  border-radius: 5px 0 0 0;
  -moz-border-radius: 5px 0 0 0;
  -webkit-border-radius: 5px 0 0 0;
}
#leftmenu > ul > li:last-child > a {
  border-radius: 0 0 0 5px;
  -moz-border-radius: 0 0 0 5px;
  -webkit-border-radius: 0 0 0 5px;
}

/* 메뉴 항목 배경 */
#leftmenu > ul > li.active a {
