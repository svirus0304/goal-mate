/* hover 시 색깔 */
#leftmenu > ul > li:hover > a {
  background-color: #ffffff;	/* 배경색 */
  /* background-image: url(data:image/png;base64,);
  background-repeat: repeat-x; */
  /* box-shadow: inset 0 0 5px black;
  -moz-box-shadow: inset 0 0 5px black;
  -webkit-box-shadow: inset 0 0 5px black; */
  color: #333333;	/* 글씨색 */
}
#leftmenu .has-sub {
  z-index: 1;
}
#leftmenu .has-sub:hover > ul {
  display: block;
}
/* 2단메뉴 설정 */
#leftmenu .has-sub ul {
  display: none;
  position: absolute;
  width: 150px;	/*  2단메뉴 가로길이*/
  top: 5px;
  left: 100%;
}
#leftmenu .has-sub ul a {
  font-size: 12px;
  background-color: #333333;
  top: 50px;
  /* width: 490px;
  height: 290px; */
}
#leftmenu .has-sub ul li {
  *margin-bottom: -1px;
}
