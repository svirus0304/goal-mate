	String club_pw; //클럽 비밀번호
	String club_area; //클럽 지역
	String club_age; //클럽 연령대 
	String club_name; //클럽명
	String club_path; //엠블렘 경로
	String club_type; //클럽 유형
	String club_phone; //대표번호
	String club_kakao; //카카오id
	int club_win; //승
	int club_draw; //무
	int club_lose; //패
	String club_used; //클럽 사용 유무
	String club_join; //클럽 가입일
	public String getClub_id() {
		return club_id;
	}
	public void setClub_id(String club_id) {
		this.club_id = club_id;
	}
	public String getClub_pw() {
		return club_pw;
	}
	public void setClub_pw(String club_pw) {
		this.club_pw = club_pw;
	}
	public String getClub_area() {
		return club_area;
	}
	public void setClub_area(String club_area) {
		this.club_area = club_area;
	}
	public String getClub_age() {
		return club_age;
	}
	public void setClub_age(String club_age) {
		this.club_age = club_age;
	}
	public String getClub_name() {
		return club_name;
	}
	public void setClub_name(String club_name) {
		this.club_name = club_name;
	}
	public String getClub_path() {
		return club_path;
	}
	public void setClub_path(String club_path) {
		this.club_path = club_path;
	}
	public String getClub_type() {
		return club_type;
	}
	public void setClub_type(String club_type) {
		this.club_type = club_type;
	}
	public String getClub_phone() {
		return club_phone;
	}
	public void setClub_phone(String club_phone) {
		this.club_phone = club_phone;
	}
	public String getClub_kakao() {
