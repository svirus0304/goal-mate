	}
	public int getClub_draw() {
		return club_draw;
	}
	public void setClub_draw(int club_draw) {
		this.club_draw = club_draw;
	}
	public int getClub_lose() {
		return club_lose;
	}
	public void setClub_lose(int club_lose) {
		this.club_lose = club_lose;
	}
	public String getClub_used() {
		return club_used;
	}
	public void setClub_used(String club_used) {
		this.club_used = club_used;
	}
	public String getClub_join() {
		return club_join;
	}
	public void setClub_join(String club_join) {
		this.club_join = club_join;
